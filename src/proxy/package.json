  "license": "AGPLv3 + noncommercial restriction",
  "devDependencies": {
    "@types/cookies": "^0.7.10",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2"
  },
  "dependencies": {
    "@types/express": "^4.17.21",
    "awaiting": "^3.0.0",
    "cookie-parser": "^1.4.6",
    "cookies": "^0.8.0",
    "debug": "^4.3.4",
    "express": "^4.18.2",
    "http-proxy": "^1.18.1"
  }
}
